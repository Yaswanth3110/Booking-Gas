<%- include('../Boxes/header') ;%>


<br><br>
<div class="container">
  <div class="table-responsive-sm">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Product</th>
          <th>Quantity</th>
          <th>status</th>
          <th>Details</th>
          <th>Date</th>
          <th>Time</th>
        </tr>
      </thead>
      <tbody>
        <% ordered_items.forEach(x => {%>
          <% for(let i = 0 ; i < Object.keys(x["order_details"]).length ; i++ ) {%>
            <tr>
              <td><%=Object.keys(x["order_details"])[i] %></td>
              <td><%=x["order_details"][Object.keys(x["order_details"])[i]] %></td>
              <td><%= x["status"]%></td>
              <td><a href="<%= x["username"]+"-"+x["_id"] %>" >See More</a></td>
              <td><%= x["ordered_date"]%></td>
              <td><%= x["ordered_time"]%></td>

            </tr>
            <%}%>
          <% }) %>
      </tbody>
    </table>

  </div>
</div>




</html>